{
	"schemaVersion": 1,
	"id": "blockhighlight",
	"version": "${version}",
	"name": "Custom Block Highlight",
	"description": "A simple mod to modify how your block outline looks. Includes animations and fancy rendering options!",
	"authors": ["Tektonikal"],
	"contact": {
		"issues": "https://discord.gg/hbcZD7meKw"
	},
	"license": "LGPL-3.0",
	"icon": "assets/icon.png",
	"environment": "client",
	"entrypoints": {
		"modmenu": [
			"tektonikal.customblockhighlight.config.ModMenuIntegration"
		],
		"main": [
			"tektonikal.customblockhighlight.Blockhighlight"
		]
	},
	"mixins": [
		"custom-block-highlight.mixins.json"
	],
	"depends": {
		"fabricloader": ">=${loader_version}",
		"fabric": "*",
		"minecraft": "${minecraft_version}",
		"yet_another_config_lib_v3": "*"
	}
}
